{
  "expo": {
    "name": "EventShareNew",
    "slug": "EventShareNew",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "QR kod taramak için kamera erişimine izin verin.",
        "CFBundleDisplayName": "EventShare",
        "CFBundleName": "EventShare"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.osmanbatmz.eventshareapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-barcode-scanner",
      [
        "expo-camera",
        {
          "cameraPermission": "QR kod taramak için kamera erişimine izin verin."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4ae93b6b-9257-40db-8fbe-66c142447907"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4ae93b6b-9257-40db-8fbe-66c142447907"
    }
  }
}
